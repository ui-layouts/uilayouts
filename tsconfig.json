{
    "extends": "@repo/typescript-config/base.json",
    "compilerOptions": {
      "jsx": "react",
      "module": "esnext",
      "target": "es6",
      "allowJs": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "isolatedModules": true,
      "resolveJsonModule": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
    }
  }
  